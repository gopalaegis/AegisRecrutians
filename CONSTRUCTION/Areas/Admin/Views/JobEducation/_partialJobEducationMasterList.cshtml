@model List<CONSTRUCTION.Areas.Admin.Model.JobEducationViewModel>

<table id="tblJobEducationMaster">
    <thead>
        <tr>
            <th>JobEducation</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.Education</td>
            <td class="light-gray text-right d-flex align-items-center">
                <i class="deleteRow deleteRow fa fa-trash-o black fa-d ml-3 pr-2" aria-hidden="true" data-id="@item.Id"></i>
                <i class="editRow fa fa-pencil creamy ml-2 pr-2" aria-hidden="true" data-id="@item.Id" data-toggle="modal" data-target="#addEducation"></i>
                @*<i class="fa fa-times red ml-2 pr-2" aria-hidden="true"></i>*@
            </td>
        </tr>
        }
    </tbody>
</table>

