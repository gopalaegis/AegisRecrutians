@model List<CONSTRUCTION.Areas.Admin.Model.JobQualicationViewModel>

<table id="tblJobQualificationMaster">
    <thead>
        <tr>
            <th>JobQualification</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Qualification</td>
                <td class="light-gray text-right d-flex align-items-center">
                    <i class="deleteRow deleteRow fa fa-trash-o black fa-d ml-3 pr-2" aria-hidden="true" data-id="@item.Id" title="Delete"></i>
                    <i class="editRow fa fa-pencil creamy ml-2 pr-2" aria-hidden="true" data-id="@item.Id" data-toggle="modal" data-target="#addQualification" title="Edit"></i>
                    @*<i class="fa fa-times red ml-2 pr-2" aria-hidden="true"></i>*@
                </td>
            </tr>
        }
    </tbody>
</table>

